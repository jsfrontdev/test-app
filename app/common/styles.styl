///media-vars///
w = 'width'
h = 'height'
wh = 'wh'
hl = 'hl'
hp = 'hp'
wl = 'wl'
who = 'who'

ip6 = 'ip6'

media(a,b,c)
	if a == 'width' || a == 'height'
		return 'screen and (max-'+ a +' '+ b +'px)'
	if a == 'wh' 
		return 'screen and (max-width '+b+'px) and (max-height '+c+'px)'
	if a == 'hl' 
		return 'screen and (max-height '+b+'px) and (orientation: landscape)'
	if a == 'wl' 
		return 'screen and (max-height '+b+'px) and (orientation: landscape)'
	if a == 'hp' 
		return 'screen and (max-height '+b+'px) and (orientation: portrait)'
	if a == 'who'
		return 'only screen and (width '+b+'px) and (height '+c+'px)'
	if a == 'ip6'
		return 'only screen and (min-device-width : 375px) and (max-device-width : 667px) and (device-aspect-ratio : 375/667) and (aspect-ratio : 667/375) and (device-pixel-ratio : 2) and (-webkit-min-device-pixel-ratio : 2)'


